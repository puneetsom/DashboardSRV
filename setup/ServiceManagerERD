--Table definition for Service Manager Contract Temp Table

create table  ServiceMgrDB_Contracts
(
customer_id  number(10),
contract_id     number(12),
sales_rep   varchar2(70),
rep_id  number(6),
sales_manager varchar2(70),
manager_id  number(6),
general_manager varchar2(70),
general_manager_id number(6),
contract_status varchar2(30),
listed_name varchar2(200),
product_name varchar2(50),
issue_date date,
atn  varchar2(20),
sfa_bots_amt number(11,2),
sfa_nisd_amt number(11,2),
vertical varchar2(50),
category varchar2(20),
ppc_ind varchar2(1),
office_code varchar2(3)
)


commit;


--Table definition for Service Manager Completed Items Temp Table


create table  ServiceMgrDB_CompletedItems
(
customer_id  number(10),
sales_rep   varchar2(70),
rep_id  number(6),
sales_manager varchar2(70),
manager_id  number(6),
general_manager varchar2(70),
general_manager_id number(6),
listed_name varchar2(200),
product_name varchar2(50),
issue_date date,
item_id number(9),
priceplan_id number(9),
udac_code varchar2(10),
heading_name varchar2(375),
atn  varchar2(20),
change_requests varchar2(50),
sfa_bots_amt number(11,2),
sfa_nisd_amt number(11,2),
region  varchar2(1),
vertical varchar2(50),
category varchar2(20),
ppc_ind varchar2(1),
udactype varchar2(20),
office_code varchar2(3)
)

commit;


--Table definition for Service Manager In Progress Items Temp Table

create table  ServiceMgrDB_InProgressItems
