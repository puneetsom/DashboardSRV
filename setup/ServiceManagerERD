product_name varchar2(50),
issue_date date,
item_id number(9),
priceplan_id number(9),
udac_code varchar2(10),
heading_name varchar2(375),
atn  varchar2(20),
sfa_bots_amt number(11,2),
sfa_nisd_amt number(11,2),
vertical varchar2(50),
category varchar2(20),
ppc_ind varchar2(1),
udactype varchar2(20),
office_code varchar2(3)
)




--Table definition for Service Manager Queried Items Temp Table

create table  ServiceMgrDB_QueriedItems
(
customer_id  number(10),
sales_rep   varchar2(70),
rep_id  number(6),
sales_manager varchar2(70),
manager_id  number(6),
general_manager varchar2(70),
general_manager_id number(6),
query_code number(4),
query_group varchar2(2),
query_level varchar2(1),
listed_name varchar2(200),
product_name varchar2(50),
issue_date date,
item_id number(9),
priceplan_id number(9),
udac_code varchar2(10),
heading_name varchar2(375),
atn  varchar2(20),
sfa_bots_amt number(11,2),
sfa_nisd_amt number(11,2),
vertical varchar2(50),
category varchar2(20),
ppc_ind varchar2(1),
udactype varchar2(20),
office_code varchar2(3)
)

commit;


create table  rt_vertical
(
vertical   varchar2(50),
Product_Type	varchar2(20),
HEADING_CODE	number(8,0),
HEADING_NAME varchar2(375),
Region	varchar2(20),
SIC_CODE	 number(4),
SIC_Title	varchar2(100),
NAICS_Title varchar2(100)
)

commit;


