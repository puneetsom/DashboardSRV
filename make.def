
ECHO=@echo

define BB_COMPILE
	$(ECHO) "---------------------------------------"
	$(ECHO) " Compile DashboardSRV " 
	$(ECHO) "---------------------------------------"
	cd $(CCMSCA)
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml clean prepare compile;\
	cd $(CCMSCA); $(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=st3 package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=st22 package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=st2 package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=st1 package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=st4 package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=stfn package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=st3 -Dloc=site package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=uat9 -Dloc=site package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=uat13 -Dloc=site package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=pet1 -Dloc=site package;\
	$(ANT_HOME)/bin/ant -f $(CCMSCA)/build.xml -Denv=pet -Dloc=site package
endef

PRE_LIBS:
	$(BB_COMPILE)



